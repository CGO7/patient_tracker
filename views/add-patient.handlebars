<div class="row container center indigo">
  <div class="col-md-6">
    <h2>New Patient Info</h2>

    <form class="form patient-form">
        <div class="form-group">
        <label for="first-name">First Name:</label>
        <input class="form-input" type="text" autocomplete="off" id="first-name"/>
        </div>
        <div class="form-group">
        <label for="last-name">Last Name:</label>
        <input class="form-input" type="text" autocomplete="off" id="last-name"/>
        </div>
        <div class="form-group">
        <label for="phone-number">Phone Number:</label>
        <input class="form-input" type="text" autocomplete="off" id="phone-number"/>
        </div>
        <div class="form-group">
        <label for="dob">Date of Birth:</label>
        <input class="form-input" type="text" autocomplete="off" id="dob"/>
        </div>
        <div class="form-group">
        <label for="gender">Gender:</label>
        <input class="form-input" type="text" autocomplete="off" id="gender"/>
        </div>
        <div class="form-group">
        <label for="drug-allergies">Drug Allergies:</label>
        <input class="form-input" type="text" autocomplete="off" id="drug-allergies"/>
        </div>
        <div class="form-group">
        <label for="insurance">Insurance:</label>
        <input class="form-input" type="text" autocomplete="off" id="insurance"/>
        </div>
        <div class="form-group">
        <label for="address">Address:</label>
        <input class="form-input" type="text" autocomplete="off" id="address"/>
        </div>
        <div class="form-group">
        <label for="city">City:</label>
        <input class="form-input" type="text" autocomplete="off" id="city"/>
        </div>
        <div class="form-group">
        <label for="state">State:</label>
        <input class="form-input" type="text" autocomplete="off" id="state"/>
        </div>
        <div class="form-group">
        <label for="zip">Zip Code:</label>
        <input class="form-input" type="text" autocomplete="off" id="zip"/>
        </div>
        <div class="form-group">
            <button class="btn btn-primary" type="submit">Submit</button>
        </div>
    </form>
  </div>

<script src="../public/js/newpatient.js"></script>